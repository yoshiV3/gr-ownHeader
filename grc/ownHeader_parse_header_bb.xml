<block>
  <name>Parse header bb</name>
  <key>ownHeader_parse_header_bb</key>
  <category>[OWNHEADER]</category>
  <import>import ownHeader</import>
  <make>ownHeader.parse_header_bb($tag_len_key, $frame_len_key, $max_symbol, $own_id, $max_padding)</make>
  <param>
    <name>Tag_len_key</name>
    <key>tag_len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <param>
    <name>Frame_len_key</name>
    <key>frame_len_key</key>
    <value>"frame_len"</value>
    <type>string</type>
  </param>
  <param>
    <name>Max_symbol</name>
    <key>max_symbol</key>
    <value>1</value>
    <type>byte</type>
  </param>
  <param>
    <name>Own_id</name>
    <key>own_id</key>
    <value>1</value>
    <type>byte</type>
  </param>
  <param>
    <name>Max_padding</name>
    <key>max_padding</key>
    <value>0</value>
    <type>byte</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
